{
  "name": "last-run-action",
  "version": "0.1.0",
  "private": true,
  "description": "A GitHub Action to determine when a workflow was last run.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --minify",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "npm run build",
    "test": "jest --passWithNoTests",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "github",
    "action",
    "last",
    "run",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^2.1.7",
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "adm-zip": "^0.5.12",
    "async-retry": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3",
    "typescript": "5.3.3",
    "@vercel/ncc": "^0.38.1"
  }
}
