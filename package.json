{
  "name": "last-run-action",
  "version": "0.1.0",
  "private": true,
  "description": "A GitHub Action to determine when a workflow was last run.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --minify",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "npm run build",
    "test": "jest --passWithNoTests",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "github",
    "action",
    "last",
    "run",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "async-retry": "^1.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "typescript": "5.9.2"
  }
}
